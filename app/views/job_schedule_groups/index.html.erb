<h1>Listing job_schedule_groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_schedule_groups.each do |job_schedule_group| %>
      <tr>
        <td><%= job_schedule_group.name %></td>
        <td><%= link_to 'Show', job_schedule_group %></td>
        <td><%= link_to 'Edit', edit_job_schedule_group_path(job_schedule_group) %></td>
        <td><%= link_to 'Destroy', job_schedule_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

