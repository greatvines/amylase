<h1>Listing birst_spaces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Client</th>
      <th>Type</th>
      <th>Space</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @birst_spaces.each do |birst_space| %>
      <tr>
        <td><%= birst_space.name %></td>
        <td><%= birst_space.client %></td>
        <td><%= birst_space.type %></td>
        <td><%= birst_space.space_id %></td>
        <td><%= link_to 'Show', birst_space %></td>
        <td><%= link_to 'Edit', edit_birst_space_path(birst_space) %></td>
        <td><%= link_to 'Destroy', birst_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Birst space', new_birst_space_path %>
