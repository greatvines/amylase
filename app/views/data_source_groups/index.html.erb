<h1>Listing data_source_groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_source_groups.each do |data_source_group| %>
      <tr>
        <td><%= data_source_group.name %></td>
        <td><%= link_to 'Show', data_source_group %></td>
        <td><%= link_to 'Edit', edit_data_source_group_path(data_source_group) %></td>
        <td><%= link_to 'Destroy', data_source_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data source group', new_data_source_group_path %>
