<h1>Listing birst_process_groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @birst_process_groups.each do |birst_process_group| %>
      <tr>
        <td><%= birst_process_group.name %></td>
        <td><%= birst_process_group.description %></td>
        <td><%= link_to 'Show', birst_process_group %></td>
        <td><%= link_to 'Edit', edit_birst_process_group_path(birst_process_group) %></td>
        <td><%= link_to 'Destroy', birst_process_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Birst process group', new_birst_process_group_path %>
