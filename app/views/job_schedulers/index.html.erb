<h1>Listing job_schedulers</h1>

<table>
  <thead>
    <tr>
      <th>Down</th>
      <th>Started at</th>
      <th>Uptime</th>
      <th>Threads</th>
      <th>Job list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_schedulers.each do |job_scheduler| %>
      <tr>
        <td><%= job_scheduler.down %></td>
        <td><%= job_scheduler.started_at %></td>
        <td><%= job_scheduler.uptime %></td>
        <td><%= job_scheduler.threads %></td>
        <td><%= job_scheduler.job_list %></td>
        <td><%= link_to 'Show', job_scheduler %></td>
        <td><%= link_to 'Edit', edit_job_scheduler_path(job_scheduler) %></td>
        <td><%= link_to 'Destroy', job_scheduler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job scheduler', new_job_scheduler_path %>
