<%= simple_form_for(@job_scheduler) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :down %>
    <%= f.input :started_at %>
    <%= f.input :uptime %>
    <%= f.input :threads %>
    <%= f.input :job_list %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
